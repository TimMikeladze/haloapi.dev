{
  "name": "halo-graphql",
  "version": "0.0.7",
  "description": "GraphQL module for the Halo Infinite API.",
  "author": "Tim Mikeladze <tim.mikeladze@gmail.com>",
  "keywords": [
    "halo",
    "halo infinite graphql",
    "halo graphql",
    "halo infinite api",
    "halo api",
    "halo graphql api",
    "halo infinite graphql api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TimMikeladze/haloapi.dev.git"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "microbundle watch src/{index}.ts",
    "build": "microbundle src/{index}.ts",
    "lint": "eslint --fix {src,__tests__}/**/*.ts",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "eslint": "8.14.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "format-graphql": "1.4.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.4.1",
    "microbundle": "0.15.0",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  },
  "peerDependencies": {},
  "dependencies": {
    "graphql-modules": "2.0.0",
    "lib": "4.3.3"
  }
}
